<div class="row">
	<h2> My Top 5 </h2>
	 <% @songs.each do |song| %>
		  	<div class="well well-lg">
		      <% if song.artist.avatar.exists? %>
                <%= image_tag song.artist.avatar.url(:thumb) %>
              <% end %>
			  <h2><%= song.name %></h2>
			  <h3>by <%= song.artist.name %> </h3>
			   <p><% "#{song.rating}".to_i.times do |rank| %>
	               <span class="glyphicon glyphicon-star"></span>
	            <% end %>
	            </p>
	    </div>
	 <% end %>
</div>

